{
    "type": "symfony-bundle",
    "name": "kherge/providence",
    "description": "Makes testing through reflection easy.",
    "keywords": ["class", "object", "method", "reflection"],
    "license": [
        "MIT",
        "Apache-2.0"
    ],
    "support": {
        "issues": "https://github.com/kherge-php/providence/issues",
        "source": "https://github.com/kherge-php/providence"
    },

    "authors": [
        {
            "name": "Kevin Herrera",
            "email": "kevin@herrera.io",
            "homepage": "http://kevin.herrera.io/",
            "role": "Developer"
        }
    ],

    "autoload": {
        "psr-4": {
            "KHerGe\\Providence\\": "src/KHerGe/Providence"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "KHerGe\\Providence\\Test\\": "tests/KHerGe/Providence/Test"
        }
    },

    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },

    "require": {
        "php": ">=7.1"
    }
}
